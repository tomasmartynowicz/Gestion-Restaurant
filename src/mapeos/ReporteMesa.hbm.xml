<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="datos.ReporteMesa" table="reporteMesa">
		<id column="idReporteMesa" name="idReporteMesa">
			<generator class="identity" />
		</id>
		<property column="fechaInicio" name="fechaInicio" type="calendar"/>
	</class>
</hibernate-mapping>